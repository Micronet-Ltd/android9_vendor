<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.qualcomm.qti.securemsm.mdtp.MdtpDemo.EmergencyDialerActivity" >

    <Space
        android:id="@+id/Space1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="fill_vertical" />

    <TextView
        android:id="@+id/letters6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button6"
        android:layout_below="@+id/button6"
        android:text="@string/MNO"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button7"
        android:layout_alignBottom="@+id/button7"
        android:layout_alignRight="@+id/button3"
        android:text="@string/dial_9"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitNineOnClick"/>

    <TextView
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button7"
        android:layout_alignBottom="@+id/button7"
        android:layout_alignLeft="@+id/button5"
        android:text="@string/dial_8"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitEightOnClick"
        android:gravity = "center_horizontal"/>

    <TextView
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/letters4"
        android:layout_alignBaseline="@+id/button4"
        android:layout_alignBottom="@+id/button4"
        android:layout_alignRight="@+id/button3"
        android:text="@string/dial_6"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitSixOnClick"/>

    <TextView
        android:id="@+id/letters4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button5"
        android:layout_below="@+id/button4"
        android:text="@string/JKL"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button4"
        android:layout_below="@+id/letters4"
        android:layout_marginTop="16dp"
        android:text="@string/dial_7"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitSevenOnClick"/>

    <TextView
        android:id="@+id/letters3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button7"
        android:layout_alignRight="@+id/button4"
        android:text="@string/GHI"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/letters2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button3"
        android:layout_below="@+id/button3"
        android:text="@string/DEF"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/letters1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/letters2"
        android:layout_alignBottom="@+id/letters2"
        android:layout_alignRight="@+id/button2"
        android:text="@string/ABC"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button1"
        android:layout_below="@+id/letters1"
        android:layout_marginTop="16dp"
        android:text="@string/dial_4"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitFourOnClick"/>

    <TextView
        android:id="@+id/letters7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button7"
        android:layout_alignLeft="@+id/button7"
        android:text="@string/PQRS"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/letters9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/letters8"
        android:layout_alignBottom="@+id/letters8"
        android:layout_alignRight="@+id/letters6"
        android:text="@string/WXYZ" />

    <TextView
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/letters1"
        android:layout_marginLeft = "100dip"
        android:text="@string/dial_1"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitOneOnClick"/>

    <TextView
        android:id="@+id/button_star"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/button7"
        android:layout_below="@+id/letters7"
        android:layout_marginTop="16dp"
        android:text="@string/dial_star"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitStarOnClick"/>

    <TextView
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button2"
        android:layout_alignBottom="@+id/button2"
        android:layout_marginLeft = "300dp"
        android:text="@string/dial_3"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitThreeOnClick"/>

    <TextView
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button4"
        android:layout_alignBottom="@+id/button4"
        android:layout_alignLeft="@+id/letters1"
        android:text="@string/dial_5"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitFiveOnClick"
        android:gravity = "center_horizontal"/>

    <TextView
        android:id="@+id/button0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button_star"
        android:layout_alignBottom="@+id/button_star"
        android:layout_alignRight="@+id/letters4"
        android:text="@string/dial_0"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitZeroOnClick"
        android:gravity = "center_horizontal"/>

    <TextView
        android:id="@+id/letters8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button_star"
        android:layout_centerHorizontal="true"
        android:text="@string/TUV"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/button_sulamit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button0"
        android:layout_alignBottom="@+id/button0"
        android:layout_alignRight="@+id/letters2"
        android:text="@string/dial_sulamit"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitSulamitOnClick"/>

    <TextView
        android:id="@+id/dial_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:hint="@string/dialer_hint"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:gravity="center_horizontal"
        android:textSize="30sp"/>

    <TextView
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/letters8"
        android:layout_below="@+id/dial_number"
        android:layout_marginTop="35dp"
        android:text="@string/dial_2"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="50sp"
        android:clickable="true"
        android:onClick="digitTwoOnClick"
        android:gravity = "center_horizontal"/>

    <TextView
        android:id="@+id/button_call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button0"
        android:layout_centerHorizontal="true"
        android:layout_alignBaseline="@+id/button_delete"
        android:layout_alignBottom="@+id/button_delete"
        android:text="@string/dialer_call"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#008000"
        android:onClick="callOnClick"/>

    <TextView
        android:id="@+id/button_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button_delete"
        android:layout_alignBottom="@+id/button_delete"
        android:layout_alignRight="@+id/button_star"
        android:text="@string/dialer_back"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:onClick="backOnClick"/>

    <TextView
        android:id="@+id/button_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button_sulamit"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/button_sulamit"
        android:text="@string/dialer_del"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:onClick="deleteOnClick"/>

</RelativeLayout>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape
            android:shape="rectangle">
        <corners
            android:radius="5dp"
            />
        <solid
            android:color="@color/stroke.gray"
            />
        <stroke
            android:width="2dp"
            android:color="@color/text.white"
            />
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
        <corners
            android:radius="5dp"
            />
        <solid
            android:color="@color/stroke.blue"
            />
        <stroke
            android:width="2dp"
            android:color="@color/text.white"
            />
        </shape>
    </item>
</selector>